function MOV = drawCircle(dobot, arduinoObj, cp, r)
    %% moves robot in a circle
    
    N = 20;
    ang = 0:2*pi/N:2*pi;
    
    for i = 1:N
        loc = [cp(1)+r*cos(ang(i));cp(2)+r*sin(ang(i));cp(3)];
        
        
        setdobotposition(dobot, loc, arduinoObj)
    
    end
    
    MOV = 1;
end
